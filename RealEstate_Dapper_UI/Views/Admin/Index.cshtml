
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AdminHeadLayoutComponentPartial")

<body>
    <div class="container-xxxl position-relative bg-white d-flex p-0">
        @await Component.InvokeAsync("_AdminSpinnerLayoutComponentPartial")
        @await Component.InvokeAsync("_AdminSideBarLayoutComponentPartial")

        <div class="content">
            @await Component.InvokeAsync("_AdminNavbarLayoutComponentPartial")

            @RenderBody()

            @await Component.InvokeAsync("_AdminFooterLayoutComponentPartial")
        </div>

        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>
    @await Component.InvokeAsync("_AdminScriptsLayoutComponentPartial")
</body>
</html>